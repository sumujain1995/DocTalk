# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'DocTalk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DocTalk

  pod 'Alamofire','~> 4.5' #For Network request
  pod 'AlamofireObjectMapper', '~> 5.0' #For Mapping JSON Responses


  target 'DocTalkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DocTalkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
